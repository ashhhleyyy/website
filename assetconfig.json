{
    "asset_paths": [
        {
            "input": "./assets/css/*",
            "output": "./assets-gen/css/",
            "loader": "css"
        },
        {
            "input": "./assets/fonts/*",
            "output": "./assets-gen/fonts/",
            "no_hash": true
        },
        {
            "input": "./assets/images/*",
            "output": "./assets-gen/images/",
            "hash_ignore": [
                "./assets/images/pfp.png"
            ],
            "just_copy": [
                "./assets/images/buttons/*"
            ],
          "loader": "image"
        },
        {
            "input": "./assets/scripts/*",
            "output": "./assets-gen/scripts/"
        }
    ]
}
